[
  {
    "name": "dicom",
    "description": "DICOM Server",
    "type": "DICOM_SERVER",
    "params": {
      "ae_title": "NUBIPACS",
      "bind": "0.0.0.0",
      "port": 11112,
      "aplication_entities": [
        {
          "ae_title": "WEASIS_AE",
          "allowed_ips": [],
